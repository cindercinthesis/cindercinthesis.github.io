{
  "plugins": ["prettier-plugin-astro", "prettier-plugin-tailwindcss"],
  "overrides": [
    {
      "files": "*.astro",
      "options": {
        "parser": "astro",
        "printWidth": 100,
        "semi": true,
        "singleQuote": false,
        "jsxSingleQuote": false,
        "bracketSpacing": true,
        "bracketSameLine": false
      }
    }
  ]
}
